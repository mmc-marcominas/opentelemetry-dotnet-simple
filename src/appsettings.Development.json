{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenTelemetry": {
    "Exporters": {
      "Console": {
        "Enabled": true
      },
      "Prometheus": {
        "Endpoint": "http://localhost:9184/metrics",
        "Enabled": true
      },
      "Zipkin": {
        "Endpoint": "http://localhost:9411/api/v2/spans",
        "ServiceName": "Open-Telemetry-Example",
        "Enabled": true
      }
    }
  }
}
